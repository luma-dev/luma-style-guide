{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Luma React",
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "importsNotUsedAsValues": "remove",
    "isolatedModules": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "preserve"
  }
}
