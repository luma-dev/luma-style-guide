{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Luma Svelte",
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES2020",
    "target": "ES2017",
    "importsNotUsedAsValues": "remove",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
